
#source
    
    fun nop(a = 1, b)
        {}

#bytecode

    PUSHFUN nop, 2;
    ASS "nop";
    POP 1;
    JUMP nop_end;
nop:
    ASS "b";
    POP 1;
    PUSHTYP;
    PUSHNNETYP;
    EQL;
    JUMPIFNOTANDPOP not_none;
    POP 1;
    PUSHINT 1;
not_none:
    ASS "a";
    POP 1;
    RETURN 0;
nop_end:
    EXIT;